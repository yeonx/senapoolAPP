<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.senapool_project">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_senapool"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_senapool_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Senapool_Project">
        <activity
            android:name=".IntroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".SettingEditMemberInfo"></activity>
        <activity android:name=".LoginPWActivity"></activity>
        <activity android:name=".LoginSignUpActivity"></activity>
        <activity android:name=".MyPlantDiaryDetailActivity"></activity>
        <activity android:name=".MyPlantEnrollActivity"></activity>
        <activity android:name=".LoginMainActivity"></activity>
        <activity
            android:name=".MyPlantDiaryWriteActivity"
            android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name=".DiaryFeedDetailActivity"></activity>
        <activity android:name=".DialogMyPlantWateringActivity"></activity>


    </application>

</manifest>